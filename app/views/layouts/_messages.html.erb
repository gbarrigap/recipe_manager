<div class="row">
  <div class="col-md-12">
    <% flash.each do |message_type, message| %>
      <div class="alert alert-<%= message_type %>">
        <%= message %>
        <a class="close" data-dismiss="alert"><i class="fa fa-times" aria-hidden="true"></i></a>
      </div>
    <% end %>
  </div>
</div>
